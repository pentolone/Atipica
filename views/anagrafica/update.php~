<?php

use yii\helpers\Html;

/* @var $this yii\web\View */
/* @var $model app\models\Anagrafica */

$this->title = 'Aggiorna anagrafica: ' . $model->nome . ' ' . $model->cognome;
$this->params['breadcrumbs'][] = ['label' => 'Anagrafica', 'url' => ['index']];
$this->params['breadcrumbs'][] = ['label' => $model->nome . ' ' . $model->cognome, => ['view', 'id' => $model->id]];
$this->params['breadcrumbs'][] = 'Update';
?>
<div class="anagrafica-update">

    <h1><?= Html::encode($this->title) ?></h1>

    <?= $this->render('_form', [
        'model' => $model,
    ]) ?>

</div>
